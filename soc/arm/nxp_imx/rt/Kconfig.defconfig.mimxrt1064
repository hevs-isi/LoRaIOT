if SOC_MIMXRT1064

config SOC
	string
	default "mimxrt1064"

config ARM_DIV
	default 1

config AHB_DIV
	default 0

config IPG_DIV
	default 3

config GPIO
	def_bool y

choice CODE_LOCATION
	default CODE_ITCM
endchoice

choice DATA_LOCATION
	default DATA_DTCM
endchoice

endif # SOC_MIMXRT1064
